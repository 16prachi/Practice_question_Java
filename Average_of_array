import java.util.Scanner;

public class average {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int ar[]=new int[n];
        for(int i=0;i<n;i++){
            ar[i]=sc.nextInt();
        }
        int sum=0;
        double result=0;
        for(int i=0;i<n;i++){
            
             sum=sum+ar[i];

        }
         result=sum/n;
        
        System.out.println(result );
    }
}
