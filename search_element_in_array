import java.util.Scanner;

public class Searchelement {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int ar[]=new int[n];
        for(int i=0;i<n;i++){
            ar[i]=sc.nextInt();
        }
        int x=sc.nextInt();
        for(int i=0;i<n;i++){
            if(x==ar[i]){
                System.out.println(i);
            }
        }
    }
}
